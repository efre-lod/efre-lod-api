  "openrefine_preview_html_text": "<html><head><meta charset=\"utf-8\" /></head>\n<body style=\"margin: 0px; font-family: Arial; sans-serif\">\n<div style=\"height: 100px; width: 320px; overflow: hidden; font-size: 0.7em\">\n<div style=\"margin-left: 5px;\">\n<a href=\"{id}\" target=\"_blank\" style=\"text-decoration: none;\">{title}</a>\n<span style=\"color: #505050;\">({endpoint})</span>\n<p>{content}</p>\n<p>{typ}</p>\n</div>\n</div>\n</body>\n</html>\n",
  "reconcile_doc": {
    "name": "LOD reconciliation for OpenRefine",
    "identifierSpace": "base",
    "schemaSpace": "http://schema.org",
    "defaultTypes": [],
    "view": {
      "url": "base/{{id}}"
    },
    "preview": {
      "height": 100,
      "width": 320,
      "url": "base/{{id}}.preview"
    },
    "extend": {
      "property_settings": [
        {
          "name": "limit",
          "label": "Limit",
          "type": "number",
          "default": 10,
          "help_text": "Maximum number of values to return per row (maximum: 1000)"
        },
        {
          "name": "type",
          "label": "Typ",
          "type": "string",
          "default": "",
          "help_text": "Which Entity-Type to use, allwed values: "
        }
      ],
      "propose_properties": {
        "service_url": "base",
        "service_path": "/reconcile/properties"
      }
    },
    "suggest": {
      "property": {
        "flyout_service_path": "/reconcile/flyout/property?id=${id}",
        "service_path": "/reconcile/suggest/property",
        "service_url": "base"
      },
      "type": {
        "flyout_service_path": "/reconcile/flyout/type?id=${id}",
        "service_path": "/reconcile/suggest/type",
        "service_url": "base"
      },
      "entity": {
        "flyout_service_path": "/reconcile/flyout/entity?id=${id}",
        "service_path": "/reconcile/suggest/entity",
        "service_url": "base"
      }
    }
  }
